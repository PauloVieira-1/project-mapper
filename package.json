{
  "name": "projectmapper",
  "displayName": "Project-Mapper",
  "description": "N/A",
  "version": "0.0.1",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "projectmapper.launch",
        "title": "Launch Project-Mapper"
      }
    ],
    "languages": [
      {
        "id": "project-mapper",
        "extensions": [
          "pm"
        ],
        "aliases": [
          "Project Mapper"
        ]
      }
    ],
    "iconThemes": [
      {
        "id": "project-mapper-theme",
        "label": "Project Mapper Icons",
        "path": "./icons/icon-theme.json"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "project-mapper",
          "title": "Project-Mapper",
          "icon": "src/icons/activity-bar.png"
        }
      ]
    },
    "views": {
      "project-mapper": [
        {
          "id": "View1",
          "name": "Project View"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx, css,scss, html, json}\""
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.19.0",
    "prettier": "^3.5.3",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "vscode": "^1.1.37"
  }
}
